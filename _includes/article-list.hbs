<h2>Latest posts:</h2>
<nav>
    <ul class="c-link-list">
        {{#each collections.post }}
            <li>
                <a href="{{ this.url }}">
                    {{ this.data.title }}
                    <span class="c-link-list__meta">
                        ({{ this.data.publish-date }})
                    </span>
                </a>
            </li>
        {{/each}}
    </ul>
</nav>
